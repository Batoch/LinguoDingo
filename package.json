  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "web-vitals": "^2.0.0"
  },
  "devDependencies": {
    "concurrently": "^6.2.0"
  }
}
